<?xml version="1.0" encoding="UTF-8"?>
<dispatch-entries>
  <!-- Send all public-facing unauthenticated traffic to the pubapi service. -->
  <dispatch>
    <url>*/rdap/*</url>
    <module>pubapi</module>
  </dispatch>
  <dispatch>
    <url>*/whois/*</url>
    <module>pubapi</module>
  </dispatch>
  <dispatch>
    <url>*/_dr/whois</url>
    <module>pubapi</module>
  </dispatch>
  <dispatch>
    <url>*/check</url>
    <module>pubapi</module>
  </dispatch>
</dispatch-entries>
